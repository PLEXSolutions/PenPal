{
    "name": "graphql-editor",
    "version": "3.6.9",
    "private": false,
    "license": "MIT",
    "description": "Visual node editor for GraphQL",
    "homepage": "https://graphqleditor.com",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "start": "webpack-dev-server --config ./sandbox/webpack.config.js --content-base ./sandbox/ --port 1568 --host 0.0.0.0 --hot --inline --open",
        "build": "tsc --build",
        "watch": "tsc --watch",
        "test": "jest",
        "e2e:test": "tsc --watch --project cypress/tsconfig.json | cypress open",
        "docs": "typedoc --mode file --out docs  --theme minimal --ignoreCompilerErrors --target ES6 ",
        "updatedeps": "npm i graphql-zeus@latest graphsource@latest",
        "lint": "eslint \"./src/**/*.{ts,js}\" --quiet --fix && prettier --write \"./src/**/*.{ts,js}\" "
    },
    "resolutions": {
        "@types/react": "^16.9.11"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/graphql-editor/graphql-editor.git"
    },
    "bugs": {
        "url": "https://github.com/graphql-editor/graphql-editor.git"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@types/classnames": "^2.2.6",
        "@types/faker": "^4.1.4",
        "@types/graphql": "^14.0.1",
        "@types/history": "4.7.5",
        "@types/jest": "^25.2.1",
        "@types/node": "^13.13.4",
        "@types/query-string": "^6.1.1",
        "@types/react": "^16.9.11",
        "@types/react-dom": "^16.9.3",
        "@types/webpack": "4.41.12",
        "@types/webpack-env": "^1.13.5",
        "@typescript-eslint/eslint-plugin": "^2.12.0",
        "@typescript-eslint/parser": "^2.12.0",
        "babel-loader": "^8.0.4",
        "css-loader": "^3.5.3",
        "cypress": "^4.5.0",
        "cz-conventional-changelog": "^3.2.0",
        "eslint": "^6.7.2",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-react": "^7.17.0",
        "file-loader": "^6.0.0",
        "html-loader": "^1.0.0-alpha.0",
        "html-webpack-plugin": "^4.3.0",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "mini-css-extract-plugin": "^0.9.0",
        "mocha": "^7.1.2",
        "monaco-editor": "^0.20.0",
        "monaco-editor-webpack-plugin": "^1.8.1",
        "prettier": "^2.0.5",
        "raw-loader": "^4.0.1",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "react-hot-loader": "^4.3.11",
        "style-loader": "^1.0.1",
        "ts-jest": "^25.4.0",
        "ts-loader": "^7.0.2",
        "typedoc": "^0.17.6",
        "typescript": "^3.9.2",
        "url-loader": "^4.1.0",
        "webpack": "^4.28.4",
        "webpack-cleanup-plugin": "^0.5.1",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.14",
        "worker-loader": "^2.0.0"
    },
    "peerDependencies": {
        "react": ">=16.12.0",
        "react-dom": ">=16.12.0",
        "monaco-editor": ">=0.19.1",
        "monaco-editor-webpack-plugin": ">=1.8.1",
        "worker-loader": ">=2.0.0",
        "file-loader": ">=5.0.2",
        "css-loader": ">=2.1.1"
    },
    "dependencies": {
        "classnames": "^2.2.5",
        "faker": "^4.1.0",
        "graphql": "^15.0.0",
        "graphql-zeus": "^2.6.6",
        "graphsource": "^1.7.3",
        "moment": "^2.21.0",
        "query-string": "^6.2.0",
        "re-resizable": "^6.1.1",
        "typestyle": "^2.0.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
